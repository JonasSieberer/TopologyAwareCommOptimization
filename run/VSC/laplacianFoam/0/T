FoamFile
{
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300; // Anfangswert f√ºr die Temperatur


boundaryField
{
    left
    {
        type            fixedValue;
        value           uniform 550;
    }

    right
    {
        type            fixedValue;
        value           uniform 50;
    }


    "(top|bottom|front|back)"
    {
        type            zeroGradient;
    }





}
